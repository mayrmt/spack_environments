#%Module 1.0
#
#  Trilinos Q4_2019 module for use with 'environment-modules' package:
#
module-whatis "Trilinos Q4_2019 compiled with gcc 9.3.0 and -O2"
conflict trilinos
prepend-path    PATH    /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0/bin
prepend-path    LD_LIBRARY_PATH /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0/lib
prepend-path    LIBRARY_PATH /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0/lib
prepend-path    INCLUDE /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0/include
prepend-path    C_INCLUDE_PATH /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0/include
prepend-path    CPLUS_INCLUDE_PATH /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0/include
prepend-path    CMAKE_PREFIX_PATH /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0
setenv TRILINOS_ROOT /home/opt/trilinos/Q4_2019/trilinos-build-openmpi-4.0.0_gcc-9.3.0
